    <div class="main container bg-white text-black d-flex justify-content-center rounded-5 shadow">
      <div class="buttons d-flex flex-column align-items-center justify-content-center">
            <button type="button" class="btn btn-auth fw-bold mt-4 border shadow">
              <a href="/auth/login">
                <img src="/svg/epost.svg" width="60px" height="60px">
                <span class="px-3">Logg inn med E-post</span>
              </a>
            </button>
            <button type="button" class="btn btn-auth fw-bold mt-4 border shadow">
